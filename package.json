{
  "name": "vscode-open-coverage-report",
  "displayName": "Open Coverage Report",
  "description": "Simple command to directly open the Instanbul coverage web page of the current file.",
  "repository": "https://github.com/LukeGeneva/vscode-open-coverage-report",
  "version": "1.0.0",
  "license": "MIT",
  "publisher": "lukegeneva",
  "engines": {
    "vscode": "^1.92.0"
  },
